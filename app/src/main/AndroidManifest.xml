<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.evampsaanga.evamptesting">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher" android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewpagers.ViewPagerMain"/>
        <activity android:name=".tablayout.TabLayoutActivity"/>
        <!--ListView Activities-->
        <activity android:name=".listviews.ListViewMainActivity"/>
        <activity android:name=".listviews.customlistview.CustomListActivity"/>
        <activity android:name=".listviews.expandablelistview.ExpandableListActivity"/>
    </application>

</manifest>
